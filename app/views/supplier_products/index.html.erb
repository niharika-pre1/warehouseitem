 <div class="container text-center">
  <h1 class="text-primary">Supplier Products</h1>
<table>
  <thead>
    <tr>
      
      <th>Product</th>
      <th>Product Quantity</th>
    </tr>
  </thead>
  <tbody>
    
    <% @supplier_products.each do |supplier_product| %>
      <tr>

        <td><div class="text-success"><%= supplier_product.product.name %></td>
       <td><%= supplier_product.product_quantity %></td>
       <td><%= button_to 'add_item', add_item_warehouse_product_path(supplier_product.product.id) %></td>

      </tr>
    <% end %>
  </tbody>
</table>