<h1>WarehouseProducts#index</h1>
<p>Find me in app/views/warehouse_products/index.html.erb</p>

<h1> Warehouse Product</h1>
<table>
  <tr>
    <th>product_quantity</th>
    
    <th colspan="3"></th>
  </tr>
 
  <% @warehouse_products.each do |warehouse| %>
    <tr>
      <td><%= warehouse.warehouse_id %></td>
      <td><%= warehouse.product_id %></td>
      <td><%= warehouse.product_quantity %></td>
       
    
    </tr>
  <% end %>
</table>


<h1>Warehouse Products</h1>
<table>
  <thead>
    <tr>
      <th>Warehouse</th>
      <th>Product</th>
      <th>Product Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% @warehouse_products.each do |warehouse_product| %>
      <tr>
        <td><%= warehouse_product.warehouse.name %></td>
        <td><%= warehouse_product.product.name %></td>
        <td><%= warehouse_product.product_quantity %></td>

      </tr>
    <% end %>
  </tbody>
</table>