 <div class="container text-center">
<h1 class="text-danger">Warehouse Dashboard</h1>
<table>
  <thead>
    <tr>
     <th>Product</th>
     <th>Remaining Quantity </th>
    </tr>
  </thead>
  <tbody>
   <% @warehouse_products.each do |warehouse_product| %>
      <tr>
      <td><div class="text-success"><%= warehouse_product.product.name %></td>
      <td><%= warehouse_product.product_quantity %></td>
      </tr>
    <% end %>
  </tbody>
